apiVersion: v1
clusters:
- cluster:
    certificate-authority-data: ${ca_crt}
    server: https://127.0.0.1:6443
  name: ${cluster_name}
contexts:
- context:
    cluster: ${cluster_name}
    user: admin
  name: default
current-context: default
kind: Config
preferences: {}
users:
- name: admin
  user:
    client-certificate-data: ${admin_crt}
    client-key-data: ${admin_key}