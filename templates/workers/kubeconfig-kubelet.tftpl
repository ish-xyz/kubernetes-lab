apiVersion: v1
clusters:
- cluster:
    certificate-authority-data: ${ca_crt}
    server: ${lb_apiserver_address}
  name: ${cluster_name}
contexts:
- context:
    cluster: ${cluster_name}
    user: system:node:${node_name}
  name: default
current-context: default
kind: Config
preferences: {}
users:
- name: system:node:${node_name}
  user:
    client-certificate-data: ${kubelet_crt}
    client-key-data: ${kubelet_key}
